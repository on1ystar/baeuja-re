{
  "name": "baeuja-express",
  "version": "1.0.0",
  "desciption": "test app for baeuja project",
  "main": "server.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server.js",
    "db:connect": "babel-node src/dbConnect.js",
    "build:server": "babel -d build --plugins transform-remove-console src",
    "build:swagger": "swagger-cli bundle ./src/swagger/swagger.yaml --outfile build/swagger/swagger.yaml --type yaml",
    "pm2:server": "pm2 start ecosystem.config.js --watch",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:reload": "pm2 reload peach-api --update-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.swmgit.org/tjdwls0607/1412s-peach.git"
  },
