{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "baeuja api server with express and typescript",
  "main": "src/server.js",
  "scripts": {
    "api-docs": "swagger-cli bundle ./src/swagger/openapi.yaml --outfile build/swagger.yaml --type yaml",
    "build": "sudo pm2 stop all && npm run api-docs && tsc && npm run pm2:reload",
    "dev:ts": "npm run api-docs && NODE_ENV=test nodemon --watch '*.ts' --exec 'ts-node' src/server.ts",
    "dev:js": "npm run api-docs &&  tsc && NODE_ENV=dev nodemon build/server.js",
    "pm2:start": "npm run api-docs && tsc && sudo pm2 start ecosystem.config.js --env production --watch '*.ts'",
    "pm2:reload": "sudo pm2 reload baeuja-api-server --update-env",
    "test:e2e": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
    "test:file": "nodemon --exec 'ts-node' src/test.ts"
  },
  "author": "on1ystar",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.7",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.0.26",
    "@types/passport": "^1.0.7",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.2.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.20.0",
    "aws-sdk": "^2.973.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^13.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "snake-case": "^3.0.4",
    "supertest": "^6.1.6",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^4.1.6",
    "waait": "^1.0.5",
    "yamljs": "^0.3.0"
  }
}
